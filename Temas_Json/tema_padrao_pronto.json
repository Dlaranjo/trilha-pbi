{
    "name": "tema personalizado",
    "$schema": "https://github.com/microsoft/powerbi-desktop-samples/blob/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.141.json",
    "dataColors": [
        "#118DFF",
        "#12239E",
        "#E66C37",
        "#6B007B",
        "#E645AB",
        "#4A2D75",
        "#1AAB40"
    ],
    "foreground": "#131312",
    "background": "#FFFFFF",
    "foregroundNeutralSecondary": "#6C6966",
    "backgroundLight": "#E5E0DA",
    "foregroundNeutralTertiary": "#ABA7A2",
    "backgroundNeutral": "#C6C2BE",
    "tableAccent": "#118DFF",
    "maximum": "#4A2D75",
    "center": "#1AAB40",
    "minimum": "#E645AB",
    "bad": "#E66C37",
    "neutral": "#6B007B",
    "good": "#12239E",
    "textClasses": {
        "title": {
            "fontFace": "DIN",
            "fontSize": 12,
            "fontWeight": "bold",
            "color": "#6C2484"
        },
        "label": {
            "fontFace": "DIN",
            "fontSize": 10,
            "fontWeight": "normal",
            "color": "#6C2484"
        }
    },
    "visualStyles": {
        "*": {
            "*":{
                "*": [{
                    "_show": false
                }]
            }
        },
        "cardVisual": {
            "*": {
                "layout": [
                    {
                        "maxTiles": 4
                    }
                ],
                "value": [
                    {
                        "$id": "default",
                        "fontSize": 25
                    }
                ]
            }
        }
    }
}